{
  "name": "moare-presentations-bootstrap",
  "version": "0.1",
  "private": "true",
  "ignore": [
    "**/*.txt",
    "codekit-config.json"
  ],
  "dependencies": {
    "jquery": "latest",
    "modernizr": "latest",
    "gsap": "latest",
    "ScrollMagic": "latest",
    "requirejs": "latest",
    "bootstrap-sass": "latest"
  },
  "exportsOverride": {
    "modernizr": {
      "lib": "modernizr.js"
    },
    "jquery": {
      "lib": "dist/jquery.js"
    },
    "bootstrap-sass": {
      "lib": "assets/javascripts",
      "../scss": "assets/stylesheets",
      "../fonts": "assets/fonts/bootstrap"
    },
    "gsap": {
      "lib": "src/uncompressed"
    },
    "ScrollMagic": {
      "lib": "scrollmagic/uncompressed"
    },
    "requirejs": {
      "lib": "require.js",
    }
  }
}
