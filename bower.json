{
  "name": "moare-presentations-bootstrap",
  "version": "0.3",
  "private": "true",
  "ignore": [
    "**/*.txt",
    "codekit-config.json"
  ],
  "dependencies": {
    "modernizr": "latest",
    "gsap": "latest",
    "ScrollMagic": "latest",
    "bootstrap-sass": "latest"
  },
  "exportsOverride": {
    "modernizr": {
      "": "modernizr.js"
    },
    "bootstrap-sass": {
        "": "assets/javascripts",
        "../../scss/lib": "assets/stylesheets",
        "../../fonts": "assets/fonts/bootstrap"
    },
    "gsap": {
      "": "src/uncompressed"
    },
    "ScrollMagic": {
      "": "scrollmagic/uncompressed"
    }
  }
}
